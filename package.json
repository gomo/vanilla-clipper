{
  "name": "vanilla-clipper",
  "version": "0.2.0",
  "private": true,
  "repository": "github:yarnaimo/vanilla-clipper",
  "license": "MIT",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "files": [
    "/bin"
  ],
  "main": "dist/index.js",
  "workspaces": [
    "functions",
    "web"
  ],
  "scripts": {
    "build": "tsc",
    "debug": "node --inspect -r ts-node/register",
    "dev": "yarn ts-node src/index.ts",
    "prepare": "yarn build",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "NODE_ENV=test jest -w1",
    "ts-node": "ts-node",
    "web:build": "NODE_ENV=production parcel build web/index.html -d public",
    "web:dev": "parcel web/index.html"
  },
  "browserslist": [
    "since 2018"
  ],
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@emotion/core": "^10.0.10",
    "@yarnaimo/rain": "latest",
    "material-colors": "^1.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-global-state": "^0.9.0",
    "rmwc": "^5.3.1",
    "signale": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/babel__core": "^7.1.1",
    "@types/node": "^8.10.0",
    "@types/parcel-bundler": "^1.12.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@yarnaimo/tss": "latest",
    "parcel-bundler": "^1.12.3",
    "react-hot-loader": "^4.8.4"
  }
}
